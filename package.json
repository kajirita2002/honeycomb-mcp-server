{
  "name": "@kajirita2002/honeycomb-mcp-server",
  "version": "1.0.7",
  "description": "MCP server for interacting with honeycomb API",
  "license": "MIT",
  "author": "kajirita2002",
  "homepage": "https://github.com/kajirita2002/honeycomb-mcp-server",
